{
  "i18n-ally.enabledFrameworks": ["react", "react-i18next"],
  "i18n-ally.pathMatcher": "{locale}/{namespace}.json",
  "i18n-ally.localesPaths": ["public/locales"],
  "i18n-ally.extract.keygenStyle": "snake_case",
  "i18n-ally.namespace": true,
  "i18n-ally.defaultNamespace": "common",
  "i18n-ally.sourceLanguage": "uk",
  "i18n-ally.displayLanguage": "en",
  "i18n-ally.annotationMaxLength": 20,
  "i18n-ally.annotationInPlace": false,
  "i18n-ally.annotationDelimiter": "|",
  "i18n-ally.keystyle": "nested",
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "stylelint.validate": ["css", "scss"],
  "stylelint.enable": true,
  "jest.autoRevealOutput": "on-run",
  "jest.jestCommandLine": "npm run test:unit:client --",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.rulers": [100],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "cSpell.words": ["aprikot", "autodocs", "creds", "Lngs", "signin", "testid"]
}
